{
  "rust-analyzer.checkOnSave.overrideCommand": [
      "cargo",
      "clippy",
      "--all-targets",
      "--message-format=json",
      "--",
      "--allow=clippy::module_inception",
      "--allow=clippy::new_ret_no_self",
      "--allow=clippy::zero_ptr",
      "--allow=clippy::new_without_default",
  ]
}